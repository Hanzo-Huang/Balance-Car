{
  "configurations": [
    {
      "name": "linux-gcc-arm64",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "USE_HAL_DRIVER",
        "STM32F103xx"
      ],
      "compilerPath": "/usr/bin/arm-none-eabi-gcc",
      "compilerArgs": [
        "-mcpu=cortex-m4",
        "-mfpu=fpv4-sp-d16",
        "-mfloat-abi=hard",
        "-mthumb"
      ],
      "configurationProvider": "ms-vscode.makefile-tools",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-arm64"
    }
  ],
  "version": 4
}